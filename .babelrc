{
  "presets": ["stage-0", "es2015", "react"],
  "plugins": [
    ["module-alias", [
      { "src": "./src/utils", "expose": "utils" },
      { "src": "./src/web", "expose": "web" },
      { "src": "./src/server", "expose": "server" },
      { "src": "./config", "expose": "config" }
    ]],
    "typecheck",
    "syntax-flow",
    "transform-flow-strip-types",
    "autobind-class-methods"
  ],
  "ignore": ["src/public"]
}
